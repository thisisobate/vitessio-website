{{- $index := slice -}}
{{- range $page := $.Site.RegularPages -}}
    {{- $index = $index | append (dict "title" $page.Title "version" (index (first 3 (split (delimit (split $page.RelPermalink "/") "," "") ",")) 2) "href" $page.Permalink  "summary" $page.Summary "path" ($page.Permalink | relURL)  ) -}}
{{- end -}}
{{- $index | jsonify -}}